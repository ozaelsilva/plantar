  object QryLimiteDistribuidor: TSQLQuery
    DataSource = dsMestre
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'PRODUTOR_ID'
        ParamType = ptInput
      end>
    SQL.Strings = (
      'SELECT proc.*,'
      '             dist.cnpj "cod_distribuidor",'
      '             dist.nome "nome_distribuidor" '
      'FROM produtor_credito proc'
      
        'INNER JOIN distribuidor dist ON (dist.distribuidor_id = proc.dis' +
        'tribuidor_id)'
      'WHERE proc.produtor_id = :PRODUTOR_ID'
      '')
    SQLConnection = dmPrincipal.SQLConnection1
    Left = 603
    Top = 111
